    "pinyin-engine": "github:oblind/pinyin-engine",
    "vue": "^2.6.10",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "laravel-mix": "^4.0.15",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.0.3"
  }
